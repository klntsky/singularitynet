  "compilerOptions": {
    "module": "es2022",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "ts-built",
    "moduleResolution": "nodenext",
    "target": "es2022",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["index.ts"]
}
